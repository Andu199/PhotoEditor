<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #B0E0E6;"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.StartController">
   <Label alignment="CENTER" layoutY="172.0" prefHeight="28.0" prefWidth="600.0"
          text="Hello and welcome to PhotoEditor!">
      <font>
         <Font name="Century Gothic" size="18.0"/>
      </font>
   </Label>
   <Label alignment="CENTER" layoutY="200.0" prefHeight="18.0" prefWidth="600.0"
          text="Please, select a photo from your computer">
      <font>
         <Font name="Century Gothic" size="12.0"/>
      </font>
   </Label>
   <Button layoutX="277.0" layoutY="306.0" mnemonicParsing="false" onAction="#selectPhoto"
           style="-fx-background-color: #FFD700;" text="Select">
      <font>
         <Font name="Berlin Sans FB Demi Bold" size="12.0"/>
      </font>
   </Button>
</AnchorPane>
